    "@nestjs/platform-socket.io": "^9.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "@nestjs/websockets": "^9.0.0",
    "@types/socket.io": "^3.0.2",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
